<div id="searchContainer">
	<div class="row">
		<div class="col-12 md">
			<form action="/spaces/search/" method="GET">
				<h1>City:</h1><br>
				<input type="text" name="city">
				<button type="submit">
			</form>
		</div>
	</div>

	<div class="row">
		<div class="col-6 md">
			{{#each spaces}}
				<div class="searchResult">
					<img src="{{this.photo}}" height="150px" width="150px">
					<h3>Address: {{this.address}}</h1>
					<h3>{{this.city}}, {{this.state}}</h1>
					<h3>{{this.zipcode}}</h1>
					<h3>${{this.price}} / month</h3>
					<h3>Available from <strong>{{this.from}}</strong> until <strong>{{this.to}}</strong></h3>
				</div>
			{{/each}}
		</div>
		<div class="col-6 md">
		</div>
	</div>
</div>
